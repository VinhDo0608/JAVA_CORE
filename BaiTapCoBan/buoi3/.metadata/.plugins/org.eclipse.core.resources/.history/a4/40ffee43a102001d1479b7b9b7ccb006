package vn.devpro.buoi_3;

import java.util.Scanner;

public class buoi_3_tren_lop {
	
	public static void divide(float a, float b) {
		if ( b != 0) {
			float c = a/b;
			System.out.printf("%.2f / %.2f  = %.4f", a, b, c);
		} else {
			System.out.println("Khong the thuc hien phep chia");
		}
	}
	
	
	public static void main(String[]args) {
		float a = 5, b = 0;
		System.out.println(a/b);
	}
}
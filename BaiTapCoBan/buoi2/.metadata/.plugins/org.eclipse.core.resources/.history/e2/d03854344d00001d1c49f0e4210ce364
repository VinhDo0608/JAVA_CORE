package vn.devpro.bai2;

import java.util.Scanner;

public class Thong_tin_hoc_sinh {
	String maHS;
	String hoDem;
	String ten;
	int ngay, thang, nam;
	String gioiTinh;
	double toan, van , anh;
	
	public void nhap() {
		Scanner sc = new Scanner(System.in);
		System.out.print("Nhap ma hoc sinh: ");
		maHS = sc.nextLine();
		System.out.print("Nhap ho dem: ");
		hoDem = sc.nextLine();
		System.out.print("Nhap ten: ");
		ten = sc.nextLine();
		System.out.print("Nhap ngay sinh: ");
		ngay = sc.nextInt();
		System.out.print("Nhap thang sinh: ");
		thang = sc.nextInt();
		System.out.print("Nhap nam sinh: ");
		nam = sc.nextInt();
	}
}
